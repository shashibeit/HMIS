
@{
    ViewBag.Title = "Followup";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="content-header">

    <div class="box box-primary">
        <div class="box-header with-border">
            <h3 class="box-title">Personal Details</h3>
        </div>

        <div class="col-md-2">
            <div class="form-group form-group-inputNadi">
                <label for="txtPatientId">Patient Id</label>
                <input disabled type="text" class="form-control input-sm" id="txtPatientId">
            </div>
        </div>
        <div class="col-md-2">
            <div class="form-group form-group-inputNadi">
                <label for="txtCaseID">Case Id</label>
                <input disabled type="text" class="form-control input-sm" id="txtCaseID">
            </div>
        </div>
        <div class="col-md-3">
            <div class="form-group form-group-inputNadi">
                <label for="inputNadi">Name</label>
                <input disabled type="text" class="form-control input-sm" id="txtPatientName">
            </div>
        </div>
        <div class="col-md-2">
            <div class="form-group form-group-inputNadi">
                <label for="txtCity">City</label>
                <input disabled type="text" class="form-control input-sm" id="txtCity">
            </div>
        </div>
        <div class="col-md-2">
            <div class="form-group form-group-inputNadi">
                <label for="txtMobileNo">Mobile No</label>
                <input disabled type="text" class="form-control input-sm" id="txtMobileNo">
            </div>
        </div>
        <div class="col-md-2">
            <div class="form-group form-group-inputNadi">
                <button id="btnProceedtoFollowup"  class="btn btn-success">Proceed &nbsp;<i class='fa fa-chevron-right'></i></button>
            </div>
        </div>


        <table id="FollowupTable" class="table table-bordered table-hover">
            <thead>
                <tr>
                    <th>Sr No.</th>
                    <th>Followup Date</th>
                    <th>Status</th>
                    <th>Completed Date</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
</section>


<script src="~/Content/js/appjs/Followup.js"></script>

